<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ChangePassWord</servlet-name>
        <servlet-class>ChangePassWord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAdminOrUser</servlet-name>
        <servlet-class>AddAdminOrUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuspendSurvey</servlet-name>
        <servlet-class>SuspendSurvey</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePassWord</servlet-name>
        <url-pattern>/ChangePassWord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAdminOrUser</servlet-name>
        <url-pattern>/AddAdminOrUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuspendSurvey</servlet-name>
        <url-pattern>/SuspendSurvey</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
