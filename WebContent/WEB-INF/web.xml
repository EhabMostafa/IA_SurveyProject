
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ChangePassWord</servlet-name>
        <servlet-class>ChangePassWord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAdminOrUser</servlet-name>
        <servlet-class>AddAdminOrUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuspendSurvey</servlet-name>
        <servlet-class>SuspendSurvey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuspendUser</servlet-name>
        <servlet-class>SuspendUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogIn</servlet-name>
        <servlet-class>LogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSurvey</servlet-name>
        <servlet-class>DeleteSurvey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOnUsername</servlet-name>
        <servlet-class>CheckOnUsername</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOnEmail</servlet-name>
        <servlet-class>CheckOnEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePassWord</servlet-name>
        <url-pattern>/ChangePassWord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAdminOrUser</servlet-name>
        <url-pattern>/AddAdminOrUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuspendSurvey</servlet-name>
        <url-pattern>/SuspendSurvey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuspendUser</servlet-name>
        <url-pattern>/SuspendUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/LogIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSurvey</servlet-name>
        <url-pattern>/DeleteSurvey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOnUsername</servlet-name>
        <url-pattern>/CheckOnUsername</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOnEmail</servlet-name>
        <url-pattern>/CheckOnEmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
